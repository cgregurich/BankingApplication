<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gregurichfinalproject.ViewCustomersWindowController">
   <children>
      <VBox layoutY="95.0" prefHeight="506.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="66.0" prefWidth="550.0" spacing="10.0">
               <children>
                  <Label text="First name" />
                  <TextField fx:id="firstNameTextField" />
               </children>
            </VBox>
            <VBox prefHeight="65.0" prefWidth="550.0" spacing="10.0">
               <children>
                  <Label text="Last name" />
                  <TextField fx:id="lastNameTextField" />
               </children>
            </VBox>
            <VBox prefHeight="101.0" prefWidth="550.0" spacing="10.0">
               <children>
                  <Label text="Address" />
                  <TextField fx:id="addressTextField" />
                  <TextField fx:id="cityStateZipTextField" />
               </children>
            </VBox>
            <VBox prefHeight="61.0" prefWidth="550.0" spacing="10.0">
               <children>
                  <Label text="Phone number" />
                  <TextField fx:id="phoneNumberTextField" />
               </children>
            </VBox>
            <VBox prefHeight="73.0" prefWidth="550.0" spacing="10.0">
               <children>
                  <Label text="Account number" />
                  <TextField fx:id="accountNumTextField" />
               </children>
            </VBox>
            <VBox prefHeight="71.0" prefWidth="550.0" spacing="10.0">
               <children>
                  <Label text="Balance" />
                  <TextField fx:id="balanceTextField" />
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="82.0" prefWidth="550.0" spacing="50.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                     <children>
                        <TextField fx:id="depositTextField" onMouseClicked="#clearLabels" promptText="Enter amount" />
                        <Button fx:id="depositButton" mnemonicParsing="false" onAction="#depositButtonClicked" text="Deposit" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                     <children>
                        <TextField fx:id="withdrawTextField" onMouseClicked="#clearLabels" promptText="Enter amount" />
                        <Button fx:id="withdrawButton" mnemonicParsing="false" onAction="#withdrawButtonClicked" text="Withdraw" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                     <children>
                        <Button fx:id="openAccountButton" mnemonicParsing="false" onAction="#openAccountButtonClicked" text="Open Account" />
                        <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save " />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                     <children>
                        <Button fx:id="updateInfoButton" mnemonicParsing="false" onAction="#updateInfoButtonClicked" text="Update Info" />
                        <Button fx:id="discardButton" mnemonicParsing="false" text="Discard" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="25.0" right="25.0" />
         </padding>
      </VBox>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
      <Button fx:id="previousButton" layoutX="473.0" layoutY="14.0" mnemonicParsing="false" onAction="#previousCustomerButtonClicked" onMouseClicked="#clearLabelsAndFields" prefWidth="61.0" text="Previous" />
      <Button fx:id="nextButton" layoutX="545.0" layoutY="14.0" mnemonicParsing="false" onAction="#nextCustomerButtonClicked" onMouseClicked="#clearLabelsAndFields" text="Next" />
      <VBox alignment="CENTER" layoutX="159.0" layoutY="39.0" prefHeight="38.0" prefWidth="283.0" spacing="10.0">
         <children>
            <Label fx:id="statusLabel" text="statusLabel" />
            <Label fx:id="amountLabel" text="amountLabel" />
         </children>
      </VBox>
   </children>
</AnchorPane>
