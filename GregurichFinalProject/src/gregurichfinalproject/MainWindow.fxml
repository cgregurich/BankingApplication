<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gregurichfinalproject.MainWindowController">
   <children>
      <VBox alignment="CENTER" prefHeight="172.0" prefWidth="220.0" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addCustomerButtonClicked" text="Add Customer" />
            <Button mnemonicParsing="false" onAction="#viewCustomersButtonClicked" text="View All Customers" />
            <Button mnemonicParsing="false" onAction="#searchCustomersButtonClicked" text="Search Customers" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="25.0" prefWidth="66.0" text="Exit">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="40.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
