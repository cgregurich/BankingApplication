<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gregurichfinalproject.ViewCustomersWindowController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
      <VBox alignment="CENTER" layoutX="-1.0" layoutY="41.0" prefHeight="560.0" prefWidth="500.0" spacing="20.0">
         <children>
            <Label fx:id="statusLabel" text="statusLabel" />
            <TextField fx:id="firstNameTextField" prefHeight="25.0" prefWidth="158.0" promptText="*First name" />
            <TextField fx:id="lastNameTextField" promptText="*Last name" />
            <TextField fx:id="phoneNumberTextField" promptText="*Phone number" />
            <TextField fx:id="streetAddressTextField" promptText="*Street address" />
            <TextField fx:id="secondaryAddressTextField" promptText="Apartment, suite, unit, building, floor, etc." />
            <TextField fx:id="cityTextField" promptText="*City" />
            <ComboBox fx:id="stateComboBox" prefHeight="25.0" prefWidth="489.0" promptText="*Select state" />
            <TextField fx:id="zipTextField" promptText="*ZIP" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" text="Save" />
                  <Button mnemonicParsing="false" text="Discard" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="40.0" right="40.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
